{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"test": {
			"inputs": [
				"src/**/*",
				"test/**/*"
			]
		},
		"dev": {
			"cache": false
		},
		"dev:test": {
			"cache": false
		},
		"release": {
			"cache": false
		},
		"preview": {
			"dependsOn": ["build"]
		},
		"@ginjou/storybook-vue#build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		}
	}
}
